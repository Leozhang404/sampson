{
  "name": "atg-prob",
  "version": "0.1.1",
  "main": "lib.js",
  "jsnext:main": "lib.es6.js",
  "scripts": {
    "build": "BABEL_ENV=dev babel-node rollup.js",
    "tape": "BABEL_ENV=test babel-tape-runner lib/**/*.test.js",
    "cover": "BABEL_ENV=test istanbul cover babel-tape-runner lib/**/*.test.js",
    "test": "npm run tape | faucet",
    "start": "npm run build"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "latest",
    "babel-preset-es2015-rollup": "latest",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^0.2.0",
    "rollup-plugin-babel": "^2.4.0"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types",
      "transform-decorators-legacy"
    ],
    "env": {
      "test": {
        "presets": [ "es2015", "stage-0" ]
      },
      "dev": {
        "presets": [ "es2015-rollup", "stage-0" ]
      }
    }
  }
}
